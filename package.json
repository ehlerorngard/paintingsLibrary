{
  "name": "paintingsLibrary",
  "version": "1.0.0",
  "description": "a tiny library of paintings",
  "engines": {
    "node": "10.15.3"
  },
  "main": "server/app.js",
  "proxy": "http://localhost:3000/",
  "scripts": {
    "server": "node server/app.js",
    "client": "node client/scripts/build.js",
    "start": "concurrently \"nodemon server/app.js\" \"npm run client\"",
    "build": "node client/scripts/build.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "installDeps": "yarn && cd client && yarn && cd ../server && yarn"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^3.5.1",
    "nodemon": "^2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ehlerorngard/paintingsLibrary.git"
  },
  "bugs": {
    "url": "https://github.com/ehlerorngard/paintingsLibrary.git"
  },
  "homepage": "https://github.com/ehlerorngard/paintingsLibrary.git",
  "dependencies": {
    "react-scripts": "^3.3.0"
  }
}
