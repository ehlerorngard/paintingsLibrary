{
  "name": "paintingsLibrary",
  "version": "1.0.0",
  "description": "a tiny library of paintings",
  "engines": {
    "node": "10.15.3"
  },
  "main": "server/app.js",
  "proxy": "http://localhost:3000/",
  "scripts": {
    "server": "node server/app.js",
    "client": "node client/scripts/build.js",
    "start": "concurrently \"nodemon server/app.js\" \"npm run client\"",
    "build": "concurrently \"node client/scripts/install.js\" \"node client/scripts/build.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "installDeps": "yarn && cd client && yarn && cd ../server && yarn"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^3.5.1",
    "nodemon": "^2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ehlerorngard/paintingsLibrary.git"
  },
  "bugs": {
    "url": "https://github.com/ehlerorngard/paintingsLibrary.git"
  },
  "homepage": "https://github.com/ehlerorngard/paintingsLibrary.git",
  "dependencies": {
    "@material-ui/core": "^4.7.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "apollo-boost": "^0.4.4",
    "graphql": "^14.5.8",
    "react": "^16.12.0",
    "react-apollo": "^3.0.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "redux": "^4.0.4"
  }
}
